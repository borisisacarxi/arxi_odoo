<?xml version="1.0" encoding="UTF-8" ?>

<odoo >
	<record model = "ir.module.category" id = "module_pc_management" >
		<field name = "name" >PC Management</field >
	</record >

	<record id = "group_user" model = "res.groups" >
		<field name = "name" >User</field >
		<field name = "category_id" ref = "module_pc_management" />
	</record >


	<record id = "group_manager" model = "res.groups" >
		<field name = "name" >Manager</field >
		<field name = "category_id" ref = "module_pc_management" />
		<field name = "implied_ids" eval = "[(4,ref('test7.group_user'))]" />
	</record >


	<record id = "group_super_manager" model = "res.groups" >
		<field name = "name" >Super Manager</field >
		<field name = "category_id" ref = "module_pc_management" />
		<field name = "implied_ids" eval = "[(4,ref('test7.group_manager'))]" />
	</record >


</odoo >