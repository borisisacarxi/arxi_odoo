<?xml version="1.0" encoding="UTF-8" ?>
<odoo >
	<record id = "quantety_wizard_action" model = "ir.actions.act_window" >
		<field name = "name" >Quantety</field >
		<field name = "res_model" >quantety.wizard</field >
		<field name = "view_mode" >form</field >
		<field name = "target" >new</field >
	</record >

	<record model = "ir.ui.view" id = "quantety_wizard_form" >
		<field name = "name" >quantety.wizard.form</field >
		<field name = "model" >quantety.wizard</field >
		<field name = "arch" type = "xml" >
			<form >
				<group >
					<field name = "quantety_field" />
<!--					<field name = "mailing_contact_id" />-->
				</group >
				<footer >
					<button string = "Ok" class = "oe_highlight" type = "object" name = "quantety_lol" />
				</footer >
			</form >
		</field >
	</record >

</odoo >